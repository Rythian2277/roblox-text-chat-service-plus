{
	"name": "New Chat Controller",
	"tree": {
		"$className": "DataModel",

		"ReplicatedStorage": {
			"$className": "ReplicatedStorage",
			"$path": "Source/Shared",
			"$ignoreUnknownInstances": true,

			"Packages": {
				"$path": "Packages"
			},

			"ClientLoaded": {
				"$className": "BindableEvent"
			},

			"GetClientLoaded": {
				"$className": "BindableFunction"
			}
		},

		"ServerScriptService": {
			"$className": "ServerScriptService",
			"$path": "Source/Server"
		},

		"ServerStorage": {
			"$className": "ServerStorage",
			"$ignoreUnknownInstances": true,

			"Packages": {
				"$path": "ServerPackages"
			},

			"SaveInStudio": {
				"$className": "BoolValue",

				"$properties": {
					"Value": true
				}
			}
		},

		"StarterPlayer": {
			"$className": "StarterPlayer",

			"StarterPlayerScripts": {
				"$className": "StarterPlayerScripts",
				"$path": "Source/Client",
				"$ignoreUnknownInstances": true
			},

			"$properties": {
				"HealthDisplayDistance": 0,
				"NameDisplayDistance": 0,
				"CameraMaxZoomDistance": 30,
				"CameraMinZoomDistance": 0.5,
				"AutoJumpEnabled": false
			}
		},

		"Players": {
			"$className": "Players",
			"$properties": {
				"RespawnTime": 0
			},
			"$ignoreUnknownInstances": true
		},

		"HttpService": {
			"$className": "HttpService",
			"$properties": {
				"HttpEnabled": true
			},
			"$ignoreUnknownInstances": true
		},

		"ReplicatedFirst": {
			"$className": "ReplicatedFirst",
			"$path": "Source/ReplicatedFirst",
			"$ignoreUnknownInstances": true
		},

		"SoundService": {
			"$className": "SoundService",
			"$properties": {
				"RespectFilteringEnabled": true
			}
		}
	}
}